<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping 
	package="com.beans">

	<class name="TopicThirdSug" table="t_topic_third_sug">
		<id name="id">
			<generator class="native"/>
		</id>

		<property name="topic_id" column="topic_id" type="java.lang.String" length="40"/>
		<property name="begin_sug" type="text" />
		<property name="mid_sug" type="text"/>
		<property name="end_sug" type="text"/>
	</class>
	
</hibernate-mapping>