<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping 
	package="com.beans">

	<class name="TopicFinalInfo" table="t_topic_final_info">
		<id name="id">
			<generator class="native"/>
		</id>
		<property name="topic_id"  column="topic_id" type="java.lang.String" length="40"/>
		<property name="pdfName" type="java.lang.String" column="pdf_name" length="20"/>
		<property name="wordName" type="java.lang.String" column="work_name" length="20"/>
		<property name="isSavePdf" type="int" column="is_save_pdf"/>
		<property name="isSaveWord" type="int" column="is_save_word"/>
	</class>
	
</hibernate-mapping>